{
  "name": "can-stache-element-ssr",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "prepare": "husky install",
    "build": "node jsdom-ssg/index.js",
    "build-prod": "node jsdom-ssg/index.js --environment prod",
    "build-debug": "node --inspect-brk jsdom-ssg/index.js",
    "build-e2e": "node jsdom-ssg/index.js --environment e2e",
    "serve": "node server.js --port 8080",
    "serve-prod": "node server.js --environment prod --port 8080",
    "serve-spa": "node server.js --environment dev --serverMode spa --port 8080",
    "serve-prod-spa": "node server.js --environment prod --serverMode spa --port 8080",
    "serve-e2e": "node server.js --environment e2e --port 8080",
    "serve-e2e-spa": "node server.js --environment e2e --serverMode spa --port 8080",
    "format": "prettier --write .",
    "e2e": "playwright test"
  },
  "steal": {
    "babelOptions": {
      "plugins": [
        "transform-class-properties"
      ]
    },
    "map": {
      "can-globals/is-node/is-node": "./mock-can-globals/is-node",
      "can-globals/is-browser-window/is-browser-window": "./mock-can-globals/is-browser-window"
    },
    "plugins": [
      "can",
      "steal-css"
    ],
    "bundle": [
      "can-stache-element-ssr/components/moo/moo",
      "can-stache-element-ssr/components/cow/cow"
    ]
  },
  "dependencies": {
    "can": "^6.6.2",
    "can-route": "^5.0.2",
    "can-stache-element": "git+ssh://git@github.com/canjs/can-stache-element#allow-static-inert-extended",
    "can-stache-route-helpers": "^2.0.0",
    "can-type": "^1.1.6",
    "can-zone": "^1.0.1",
    "can-zone-jsdom": "git+ssh://git@github.com/canjs/can-zone-jsdom#jsdom-19-update",
    "done-ssr": "^3.3.1",
    "fs-extra": "^10.1.0",
    "jsdom": "^20.0.1",
    "steal": "^2.3.0",
    "w3c-xmlhttprequest": "^3.0.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/canjs/can-stache-element-ssr.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/canjs/can-stache-element-ssr/issues"
  },
  "homepage": "https://github.com/canjs/can-stache-element-ssr#readme",
  "engines": {
    "node": ">=14",
    "npm": ">=6.0.0 <7"
  },
  "devDependencies": {
    "@playwright/test": "^1.27.1",
    "can-globals": "^1.2.2",
    "express": "^4.18.2",
    "http-server": "^14.1.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "optimist": "^0.6.1",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "steal-css": "^1.3.2",
    "steal-tools": "^2.3.0"
  }
}
