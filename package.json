{
  "name": "can-stache-element-ssr",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "prepare": "husky install",
    "build": "node jsdom-ssr/index.js",
    "build-prod": "node jsdom-ssr/index.js --prod true",
    "build-debug": "node --inspect-brk jsdom-ssr/index.js",
    "serve": "node server.js --port 8080",
    "serve-dev": "node server.js --dev true --port 8080",
    "serve-prod": "node server.js --prod true --port 8080",
    "format": "prettier --write ."
  },
  "steal": {
    "babelOptions": {
      "plugins": [
        "transform-class-properties"
      ]
    },
    "plugins": [
      "can",
      "steal-css"
    ]
  },
  "dependencies": {
    "can": "^6.6.2",
    "can-route": "^5.0.2",
    "can-stache-element": "git+ssh://git@github.com/canjs/can-stache-element#allow-static-inert",
    "can-stache-route-helpers": "^2.0.0",
    "can-type": "^1.1.6",
    "can-zone": "^1.0.1",
    "can-zone-jsdom": "git+ssh://git@github.com/canjs/can-zone-jsdom#jsdom-19-update",
    "done-ssr": "^3.3.1",
    "fs-extra": "^10.1.0",
    "jsdom": "^20.0.1",
    "steal": "^2.3.0",
    "w3c-xmlhttprequest": "^3.0.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/canjs/can-stache-element-ssr.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/canjs/can-stache-element-ssr/issues"
  },
  "homepage": "https://github.com/canjs/can-stache-element-ssr#readme",
  "engines": {
    "node": ">=14",
    "npm": ">=6.0.0 <7"
  },
  "devDependencies": {
    "express": "^4.18.2",
    "http-server": "^14.1.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "optimist": "^0.6.1",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "steal-css": "^1.3.2",
    "steal-tools": "^2.3.0"
  }
}
